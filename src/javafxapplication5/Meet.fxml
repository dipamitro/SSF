<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="376.0" prefWidth="600.0" style="-fx-background-color: #03203C;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication5.MeetController">
   <children>
      <AnchorPane prefHeight="398.0" prefWidth="182.0" style="-fx-background-color: #23C4ED;">
         <children>
            <VBox layoutX="41.0" layoutY="78.0" prefHeight="277.0" prefWidth="142.0" style="-fx-background-color: #23C4ED;">
               <children>
                  <Button mnemonicParsing="false" onAction="#sendButtonOnClick" prefHeight="41.0" prefWidth="98.0" text="Send">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Times New Roman" size="20.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#backButtonOnClick" prefHeight="41.0" prefWidth="98.0" text="Back">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Times New Roman" size="20.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <TextArea fx:id="textareaa" layoutX="234.0" layoutY="52.0" prefHeight="298.0" prefWidth="324.0" promptText="Type here Meeting Time and Meet Link........" wrapText="true">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </TextArea>
   </children>
</AnchorPane>
