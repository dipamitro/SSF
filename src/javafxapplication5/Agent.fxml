      <Button fx:id="button" layoutX="258.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="31.0" prefWidth="86.0" text="Login" />
      <PasswordField fx:id="textPassword" layoutX="183.0" layoutY="169.0" onMouseClicked="#passwordFieldOnClick" prefHeight="31.0" prefWidth="236.0" promptText="Enter Password" />
      <Label fx:id="notificationLabel" layoutX="136.0" layoutY="341.0" prefHeight="39.0" prefWidth="329.0" textAlignment="CENTER" textFill="RED" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="469.0" layoutY="348.0" onMouseClicked="#forgotPasswordLabelOnMouseClicked" prefHeight="25.0" prefWidth="129.0" style="-fx-background-color: GREY;" text="Forgot Password?" textFill="#573d3d">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="77.0" layoutY="31.0" prefHeight="53.0" prefWidth="476.0" text="Big Business Needs Big Security" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="32.0" />
         </font>
      </Label>
      <Button layoutX="-6.0" layoutY="326.0" mnemonicParsing="false" onAction="#previousSceneButtonOnClick" prefHeight="31.0" prefWidth="111.0" text="Previous Scene">
         <font>
            <Font size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
